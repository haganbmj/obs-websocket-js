{
  "name": "obs-websocket-js",
  "version": "0.8.0",
  "description": "OBS Websocket API in Javascript, consume @Palakis/obs-websocket",
  "main": "es5/index.js",
  "repository": "haganbmj/obs-websocket-js",
  "repoUrl": "https://github.com/haganbmj/obs-websocket-js",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:web": "webpack",
    "build:es5": "babel lib -d es5",
    "watch": "webpack --watch",
    "test": "npm run static && nyc ava",
    "ava": "nyc ava",
    "report": "nyc report --reporter=text-lcov",
    "node-coveralls": "npm run report | coveralls",
    "static": "eslint ."
  },
  "keywords": [
    "obs",
    "studio",
    "websocket",
    "node",
    "node.js"
  ],
  "author": "Brendan Hagan (haganbmj)",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "debug": "^3.0.0",
    "sha.js": "^2.4.8",
    "ws": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babili-webpack-plugin": "^0.1.1",
    "coveralls": "^2.13.1",
    "eslint": "^4.1.0",
    "eslint-config-xo-space": "^0.16.0",
    "eslint-plugin-ava": "^4.2.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.1.0",
    "webpack": "^3.5.0"
  },
  "bugs": {
    "url": "https://github.com/haganbmj/obs-websocket-js/issues"
  },
  "homepage": "https://github.com/haganbmj/obs-websocket-js#readme",
  "eslintConfig": {
    "extends": "xo-space/esnext"
  },
  "ava": {
    "files": [
      "test/*.spec.js",
      "!setup/environment.js"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          },
          "debug": true,
          "include": [
            "transform-async-to-generator"
          ],
          "exclude": [
            "transform-regenerator"
          ]
        }
      ]
    ]
  }
}
